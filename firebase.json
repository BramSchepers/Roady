{
  "hosting": {
    "public": "build/hosting",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/auth",
        "destination": "/auth/index.html"
      },
      {
        "source": "/auth/**",
        "destination": "/auth/index.html"
      }
    ]
  }
}